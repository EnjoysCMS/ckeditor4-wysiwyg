{
  "name": "enjoyscms/ckeditor4",
  "description": "Wysiwyg CKEditor4 for EnjoysCMS",
  "require": {
    "php": "^8.1",
    "enjoyscms/core": "^6.0@dev"
  },
  "autoload": {
    "psr-4": {
      "EnjoysCMS\\ContentEditor\\CKEditor4\\": "src/",
      "EnjoysCMS\\ContentEditor\\CKEditor4\\Composer\\Scripts\\": "scripts/"
    }
  },
  "scripts": {
      "ckeditor4-assets-install": "EnjoysCMS\\ContentEditor\\CKEditor4\\Composer\\Scripts\\AssetsInstallCommand",
      "post-install-cmd": [
          "echo -e \"\\x1b[1mCKEditor 4\\x1b[00m\"",
          "@ckeditor4-assets-install"
      ],
      "post-update-cmd": [
          "echo -e \"\\x1b[1mCKEditor 4\\x1b[00m\"",
          "@ckeditor4-assets-install"
      ]
  }
}
